{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "1.0.2",
    "homepage_url": "https://github.com/c-yan/open-all-urls-in-the-clipboard",
    "default_locale": "en",
    "applications": {
        "gecko": {
            "strict_min_version": "57.0"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "activeTab",
        "tabs",
        "menus",
        "clipboardRead"
    ]
}
